{
    "manifest_version": 3,
    "name": "Widescreen Praktomat",
    "version": "1.0",
    "description": "Allows the Praktomat to fully utilize the available screenspace",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["https://praktomat-emden.de/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "48": "icon.png"
    },
    "action": {
        "default_icon": {
            "48": "icon.png"
        }
    }
}

